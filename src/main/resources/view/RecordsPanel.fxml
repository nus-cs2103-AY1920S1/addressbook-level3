<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="eventName" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #30336B;" text="eventName" textFill="#e056fd" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="16.0" left="32.0" right="16.0" top="16.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <ScrollPane fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;" VBox.vgrow="ALWAYS">
         <content>
            <VBox fx:id="recordContents" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;">
               <children>
                  <Label fx:id="noContent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #ffffff;" text="Label" wrapText="true">
                     <padding>
                        <Insets bottom="16.0" left="32.0" right="16.0" top="16.0" />
                     </padding>
                  </Label>
               </children></VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
