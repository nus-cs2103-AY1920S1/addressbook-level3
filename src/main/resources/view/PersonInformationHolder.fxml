<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="informationHolder" alignment="CENTER_LEFT" minHeight="105" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="5" left="15" right="5" top="5" />
   </padding>
   <children>
      <Label fx:id="name" style="-fx-wrap-text: true;" styleClass="cell_big_label" text="\$first" />
      <FlowPane fx:id="tags" />
      <Label fx:id="nric" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$nric" />
      <Label fx:id="phone" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$phone" />
      <Label fx:id="address" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$address" />
      <Label fx:id="email" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$email" />
      <Label fx:id="dateOfBirth" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$dob" />
      <Label fx:id="gender" style="-fx-wrap-text: true;" styleClass="cell_small_label" text="\$gender" />
   </children>
</VBox>
