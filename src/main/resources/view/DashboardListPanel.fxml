         <font>
            <Font name="Devanagari Sangam MN" size="37.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
      <HBox maxHeight="-Infinity" prefHeight="70.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
