                     </opaqueInsets>
                  </VBox>
                  <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                     <image>
                        <Image url="@../images/appIcon.png" />
                     </image>
                     <GridPane.margin>
                        <Insets left="20.0" top="20.0" />
                     </GridPane.margin>
                  </ImageView>
                  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <StackPane HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Label" textFill="WHITE" />
                           </children></StackPane>
                        <Separator blendMode="HARD_LIGHT" orientation="VERTICAL" prefHeight="200.0" style=": ;" />
                        <StackPane HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Label" textFill="WHITE" />
                           </children></StackPane>
                     </children>
                     <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </padding>
                  </HBox>
                  <VBox alignment="CENTER" GridPane.rowIndex="1">
                     <children>
                        <StackPane>
                           <children>
                              <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/selectCircle.png" />
                                 </image>
                              </ImageView>
                              <ImageView fitHeight="85.0" fitWidth="85.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/flashcardIcon.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </StackPane>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <ImageView fitHeight="120.0" fitWidth="120.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/selectCircle.png" />
                                 </image>
                              </ImageView>
                              <ImageView fitHeight="85.0" fitWidth="85.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
                                 <image>
                                    <Image url="@../images/notesIcon.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </StackPane>
                        <StackPane>
                           <children>
                              <ImageView fitHeight="120.0" fitWidth="120.0" opacity="0.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/selectCircle.png" />
                                 </image>
                              </ImageView>
                              <ImageView fitHeight="85.0" fitWidth="85.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/cheatsheetIcon.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </StackPane>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="25.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
