@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title After command "add|Strawberry|3/2/2020|1"

package States <<rectangle>> {
    class State1 as "__xp0:Xpire__"
    class State2 as "__xp1:Xpire__"
    class State3 as "__xp2:Xpire__"
}

State1 -[hidden]right-> State2
State2 -[hidden]right-> State3

class Pointer as "Current State" #FFFFF

Pointer -up-> State3
@end
