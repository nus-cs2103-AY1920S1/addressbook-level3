<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>


<ScrollPane id="performance-overview" fx:id="cardPane" maxHeight="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox id="performance-overview">
         <children>
            <Label id="performance-overview-title" fx:id="title" text="\$title" />
            <VBox fx:id="chartWrapper" />
            <VBox fx:id="performanceCardsWrapper" />
         </children>
      </VBox>
   </content>
</ScrollPane>
