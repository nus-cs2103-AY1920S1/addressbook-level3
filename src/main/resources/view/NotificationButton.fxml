<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="buttonPane" maxHeight="22.0" maxWidth="22.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="buttonIcon" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets top="1.0" />
         </StackPane.margin>
      </ImageView>
      <Button fx:id="notifButton" maxHeight="20.0" maxWidth="22.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="22.0" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets top="1.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
