<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" styleClass="Sidebar" stylesheets="@Sidebar.css" translateX="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="itineraryLabel" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#handleEnterItinerary" text="Itinerary" textFill="WHITE" wrapText="true">
         <font>
            <Font size="6.0" />
         </font>
         <graphic>
            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/navbar_icons/schedule_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="expenseLabel" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#handleEnterExpenseManager" text="Expense" textFill="WHITE" wrapText="true">
         <font>
            <Font size="6.0" />
         </font>
         <graphic>
            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/navbar_icons/expense_icon.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="diaryLabel" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#handleEnterDiary" text="Diary" textFill="WHITE" wrapText="true">
         <font>
            <Font size="6.0" />
         </font>
         <graphic>
            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/navbar_icons/diary_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="contactsLabel" contentDisplay="GRAPHIC_ONLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#handleOpenHelp" text="Help" textFill="WHITE" wrapText="true">
         <font>
            <Font size="6.0" />
         </font>
         <graphic>
            <ImageView fitHeight="60.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/navbar_icons/help_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</VBox>
