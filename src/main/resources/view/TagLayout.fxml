<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="tagIcon" fitHeight="13.0" fitWidth="13.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="tagName" text="Label">
         <padding>
            <Insets bottom="1.0" left="6.0" right="3.0" top="1.0" />
         </padding>
         <font>
            <Font size="11.0" />
         </font>
      </Label>
   </children>
</HBox>
