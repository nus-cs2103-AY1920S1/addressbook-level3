<?xml version="1.0" encoding="UTF-8"?>
<!-- @@author CarbonGrid -->

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <GridPane HBox.hgrow="ALWAYS">
        <VBox alignment="CENTER_LEFT" minHeight="105" GridPane.columnIndex="0">
            <padding>
                <Insets top="5" right="5" bottom="5" left="15"/>
            </padding>
            <HBox spacing="5" alignment="CENTER_LEFT">
                <Label fx:id="id" styleClass="cell_big_label"/>
                <Label fx:id="refId" text="\$first" styleClass="cell_big_label"/>
                <Label fx:id="timing" text="\$second" styleClass="cell_big_label"/>
            </HBox>
            <Label fx:id="status" text="\$third" styleClass="cell_big_label"/>
        </VBox>
    </GridPane>
</HBox>
