<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<HBox fx:id="semesterCardPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
        </rowConstraints>
        <VBox fx:id="tagListCard" alignment="CENTER_LEFT" GridPane.columnIndex="0">
            <padding>
                <Insets bottom="5" left="5" right="5" top="5"/>
            </padding>
            <StackPane fx:id="tags" alignment="CENTER_LEFT" VBox.vgrow="ALWAYS"/>
        </VBox>
    </GridPane>
</HBox>
