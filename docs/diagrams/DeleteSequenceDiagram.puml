@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":CommandLineParser" as CommandLineParser LOGIC_COLOR
participant ":AccountDeleteCommandParser" as AccountDeleteCommandParser LOGIC_COLOR
participant "d:AccountDeleteCommand" as AccountDeleteCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("account delete 1")
activate LogicManager

LogicManager -> CommandLineParser : parseCommand("account delete 1")
activate CommandLineParser

create AccountDeleteCommandParser
CommandLineParser -> AccountDeleteCommandParser
activate AccountDeleteCommandParser

AccountDeleteCommandParser --> CommandLineParser
deactivate AccountDeleteCommandParser

CommandLineParser -> AccountDeleteCommandParser : parse("1")
activate AccountDeleteCommandParser

create AccountDeleteCommand
AccountDeleteCommandParser -> AccountDeleteCommand
activate AccountDeleteCommand

AccountDeleteCommand --> AccountDeleteCommandParser : d
deactivate AccountDeleteCommand

AccountDeleteCommandParser --> CommandLineParser : d
deactivate AccountDeleteCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AccountDeleteCommandParser -[hidden]-> CommandLineParser
destroy AccountDeleteCommandParser

CommandLineParser --> LogicManager : d
deactivate CommandLineParser

LogicManager -> AccountDeleteCommand : execute(model)
activate AccountDeleteCommand

AccountDeleteCommand -> Model : deleteAccount(1)
activate Model

Model --> AccountDeleteCommand
deactivate Model

create CommandResult
AccountDeleteCommand -> CommandResult
activate CommandResult

CommandResult --> AccountDeleteCommand
deactivate CommandResult

AccountDeleteCommand --> LogicManager : result
deactivate AccountDeleteCommand

[<--LogicManager
deactivate LogicManager
@enduml
