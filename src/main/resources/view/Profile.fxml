<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="profile" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="displayPicture" fitHeight="76.0" fitWidth="76.0" pickOnBounds="true" preserveRatio="true" />
      <VBox maxHeight="1.7976931348623157E308" prefHeight="30.0" prefWidth="314.0">
         <children>
            <Label fx:id="name" maxWidth="1.7976931348623157E308" text="Label" VBox.vgrow="ALWAYS" styleClass="profile-name"/>
            <Label fx:id="asl" maxWidth="1.7976931348623157E308" text="Label" VBox.vgrow="ALWAYS" styleClass="profile-asl" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
   </padding>
</HBox>
