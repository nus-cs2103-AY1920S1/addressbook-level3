<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="progressBarPlaceholder" maxWidth="Infinity" alignment="CENTER_LEFT" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <VBox alignment="CENTER_LEFT" fillWidth="true" minWidth="50.0" HBox.hgrow="NEVER">
         <children>
          <Label fx:id="progress" minWidth="80.0" wrapText="true" />
         </children>
      </VBox>
      <VBox alignment="CENTER_RIGHT" minWidth="280.0" HBox.hgrow="ALWAYS">
         <children>
            <ProgressBar fx:id="progressBar" maxWidth="Infinity" minWidth="250.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="25.0" progress="0.9" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
  </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
