<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:id="panel">
    <AnchorPane>
        <Label text="Reminders" textFill="ivory" fx:id="title" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
    </AnchorPane>
    <ScrollPane fx:id="reminderScrollPane">
        <VBox prefHeight="400.0" fx:id="reminderList">
        </VBox>
    </ScrollPane>
</VBox>
