<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="cardPane" prefHeight="644.0" prefWidth="501.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="644.0" prefWidth="501.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="name" prefWidth="500.0" styleClass="infoPanel_name" text="$name" textOverrun="WORD_ELLIPSIS" wrapText="true">
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
               <tooltip>
                  <Tooltip minHeight="100" minWidth="100" styleClass="tooltip" text="Empty Tooltip" />
               </tooltip>
            </Label>
            <HBox prefHeight="50.0" prefWidth="190.0">
               <children>
                  <ImageView fx:id="dateImage" fitHeight="65.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/calendar.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="date" styleClass="infoPanel_properties" text="$date" wrapText="true">
                     <padding>
                        <Insets left="5.0" top="15.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="190.0">
               <children>
                  <ImageView fx:id="caloriesImage" fitHeight="65.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/calories.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="calories" styleClass="infoPanel_properties" text="$calories" wrapText="true">
                     <padding>
                        <Insets left="5.0" top="15.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="190.0">
               <children>
                  <ImageView fx:id="quantityAndUnitImage" fitHeight="65.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/stretching-exercises.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="quantityAndUnit" styleClass="infoPanel_properties" text="$quantityAndUnit" wrapText="true">
                     <padding>
                        <Insets left="5.0" top="15.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="190.0">
               <children>
                  <ImageView fx:id="muscleImage" fitHeight="65.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/images/muscles.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <FlowPane fx:id="muscles" prefHeight="75.0" prefWidth="303.0">
                     <padding>
                        <Insets left="5.0" top="15.0" />
                     </padding></FlowPane>
               </children>
            </HBox>
            <VBox prefHeight="251.0" prefWidth="502.0">
               <children>
                  <Label styleClass="infoPanel_customProperties_label" text="Other properties:">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <StackPane fx:id="customProperties" prefHeight="202.0" prefWidth="502.0" styleClass="infoPanel_customProperties_listView" />
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
