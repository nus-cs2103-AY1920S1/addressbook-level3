<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20" prefWidth="200" />
        </columnConstraints>
        <VBox id="labels" fx:id="labels" alignment="CENTER_LEFT" minHeight="105" style="-fx-background-color: #3F3D48;" GridPane.columnIndex="0">
            <padding>
                <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <HBox id="details" alignment="CENTER_LEFT" spacing="5">
                <Label id="index" fx:id="index" />
                <Label fx:id="commandString" styleClass="cell_big_label" text="\$first" textFill="WHITE">
               <font>
                  <Font name="Arial" size="16.0" />
               </font></Label>
            </HBox>
            <FlowPane id="tags" />
        </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </GridPane>
</HBox>
