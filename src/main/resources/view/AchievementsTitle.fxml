<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" fx:id="achievementsTitle" HBox.hgrow="ALWAYS"
      xmlns="http://javafx.com/javafx/8">
    <children>
        <ScrollPane fx:id="scrollPane" minHeight="62" fitToWidth="true" pannable="true" styleClass="commonScrollPane"
                    HBox.hgrow="ALWAYS">
            <content>
                <VBox maxHeight="Infinity">
                    <children>
                        <Label fx:id="achievementsTitleLabel" alignment="TOP_LEFT" contentDisplay="TOP"
                               maxHeight="Infinity" styleClass="profile-name" text="Label" wrapText="true"
                               VBox.vgrow="always">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Label>
                        <Label fx:id="achievementsTitleDescLabel" alignment="TOP_LEFT" contentDisplay="TOP"
                               maxHeight="Infinity" styleClass="profile-asl" text="Label" wrapText="true"
                               VBox.vgrow="always">
                            <VBox.margin>
                                <Insets top="7.0"/>
                            </VBox.margin>
                        </Label>
                    </children>
                    <padding>
                        <Insets left="5.0"/>
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</HBox>
