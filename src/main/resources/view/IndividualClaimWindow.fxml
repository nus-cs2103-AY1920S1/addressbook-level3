<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.stage.Stage?>

<?import javafx.scene.layout.VBox?>
<fx:root fx:id="root" resizable="false" title="IndividualClaim" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <scene>
    <Scene onMouseClicked="#closeAction">
         <VBox fx:id="box" alignment="CENTER">
            <children>
                <Label fx:id="name" text="Label">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="date" text="Label">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="amount" text="Label">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="description" text="Label">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Label>
            </children>
            <opaqueInsets>
               <Insets bottom="100.0" left="90.0" right="90.0" top="100.0" />
            </opaqueInsets>
            <padding>
               <Insets bottom="100.0" left="90.0" right="90.0" top="100.0" />
            </padding>
         </VBox>
    </Scene>
  </scene>
</fx:root>
