<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<VBox fx:id="dailyCalendarEntries"  VBox.vgrow="ALWAYS" minHeight="0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets top="5"></Insets>
    </padding>
    <Label fx:id="day" text="/$day" styleClass="cell_big_label">
        <padding>
            <Insets left="2" top="5"></Insets>
        </padding>
    </Label>
    <ListView fx:id="dailyCalendarEntryList">
    </ListView>

</VBox>
