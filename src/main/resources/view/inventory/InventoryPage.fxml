<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane>
         <children>
            <TableView fx:id="tableView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="0.0">
              <columns>
                  <TableColumn fx:id="indexColumn" prefWidth="75.0" text="Column X" />
                <TableColumn fx:id="nameColumn" editable="false" prefWidth="75.0" sortable="false" styleClass="-fx-background-color:lightcoral-fx-background-color:lightcoral" text="NAME" />
                  <TableColumn fx:id="isDoneColumn" editable="false" prefWidth="75.0" sortable="false" styleClass="-fx-background-color:lightcoral-fx-background-color:lightcoral" text="ISDONE" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </center>
   <left>
      <VBox fx:id="sideBarLeft" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <VBox fx:id="sideBarRight" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
