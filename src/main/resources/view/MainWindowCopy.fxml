      <VBox>
        <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
          <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
          </Menu>
        </MenuBar>
            <GridPane alignment="CENTER" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="236.59998168945313" minHeight="10.0" prefHeight="83.20000610351562" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="433.4000076293945" minHeight="10.0" prefHeight="415.9999938964844" vgrow="SOMETIMES" />
