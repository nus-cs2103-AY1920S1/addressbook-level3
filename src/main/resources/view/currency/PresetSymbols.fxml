<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefWidth="200.0">
         <children>
            <ToggleButton fx:id="option1" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option1">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="option2" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option2">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="option3" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option3">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ToggleButton>
            <ToggleButton fx:id="option4" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option4">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ToggleButton>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox id="cardPane" fx:id="cardPane" minWidth="-Infinity">
         <ToggleButton fx:id="option5" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option5">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin>
         </ToggleButton>
         <ToggleButton fx:id="option6" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option6">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin>
         </ToggleButton>
         <ToggleButton fx:id="option7" mnemonicParsing="false" onMouseClicked="#handleToggle" prefWidth="50.0" text="\$option7">
            <HBox.margin>
               <Insets bottom="10.0" right="10.0" />
            </HBox.margin>
         </ToggleButton>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
