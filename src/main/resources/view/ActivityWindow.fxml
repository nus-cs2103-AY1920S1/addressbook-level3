<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <TabPane fx:id="activityWindow" tabClosingPolicy="UNAVAILABLE">
           <tabs>
               <Tab text="EmptyTab">
                   <content>
                       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                   </content>
               </Tab>
               <Tab text="FlashcardTab">
                    <fx:include fx:id="flashcardTabWindow" source="FlashcardTabWindow.fxml" />
               </Tab>
               <Tab text="NotesTab">
                   <fx:include fx:id="notesTabWindow" source="NotesTabWindow.fxml" />
               </Tab>
               <Tab text="CheatsheetTab">
                   <fx:include fx:id="cheatsheetTabWindow" source="CheatsheetTabWindow.fxml" />
               </Tab>
           </tabs>
       </TabPane>
   </children>
</StackPane>
