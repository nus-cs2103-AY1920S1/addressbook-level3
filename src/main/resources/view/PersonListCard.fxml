<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.StackPane?>
<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
    </columnConstraints>
    <VBox alignment="CENTER_LEFT" minHeight="85" GridPane.columnIndex="0">
      <padding>
        <Insets top="5" right="5" bottom="5" left="10" />
      </padding>
      <HBox spacing="15" alignment="CENTER_LEFT">
        <StackPane id="personId" fx:id="personId"></StackPane>
        <VBox spacing="5">
          <Label id="name" fx:id="name" text="\$first" styleClass="cell_big_label" />
          <FlowPane fx:id="tags" />
        </VBox>
      </HBox>
    </VBox>
  </GridPane>
</HBox>
