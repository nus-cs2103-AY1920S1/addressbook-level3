<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox id="cardPane" fx:id="cardPane" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="description" style="-fx-font-weight: bold;" text="\$commandName" />
<Label fx:id="name" styleClass="cell_big_label" text="\$commandSyntax" />
<Label fx:id="phone" styleClass="cell_small_label" text="\$commandExample" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
