<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>

<HBox id="groupCardPane" fx:id="groupCardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox alignment="CENTER_LEFT" minHeight="85" GridPane.columnIndex="0">
        <padding>
            <Insets top="5" right="5" bottom="5" left="10" />
        </padding>
        <HBox spacing="15" alignment="CENTER_LEFT">
            <StackPane fx:id="groupId" id="groupId" />
            <Label fx:id="groupName" styleClass="cell_big_label"></Label>
        </HBox>
    </VBox>
</GridPane>
</HBox>
