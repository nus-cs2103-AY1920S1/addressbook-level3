<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView styleClass="hide_scrollbar" fx:id="visitTasks" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    <columns>
        <TableColumn fx:id="indexColumn" text="Index" sortable="false" minWidth="50" maxWidth="50"/>
        <TableColumn fx:id="descriptionColumn" text="Description" sortable="false"/>
        <TableColumn fx:id="detailColumn" text="Details" sortable="false"/>
        <TableColumn fx:id="finishedColumn" text="Finished" sortable="false" minWidth="75" maxWidth="75"/>
    </columns>
</TableView>
