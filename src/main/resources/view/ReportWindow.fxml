<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label id="lbl" text="Documents Explorer" />
        <AnchorPane prefHeight="27.0" prefWidth="186.0">
            <HBox alignment="BOTTOM_RIGHT" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                <Button id="btn-refresh" fx:id="btnRefresh" onMouseClicked="#refreshTreeView" styleClass="function_btn" />
                <Button fx:id="btnDelete" alignment="BOTTOM_RIGHT" maxHeight="10.0" maxWidth="10.0" styleClass="function_btn" onMouseClicked="#deleteDocument"/>
            </HBox>
             <Label id="lbl" fx:id="lblStatus" textAlignment="RIGHT" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0" />
        </AnchorPane>
        <TreeView fx:id="treeViewReports" prefHeight="250" />
    </children>
</VBox>
